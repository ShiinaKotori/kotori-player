<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="mediaBufferingProgressBar">
        <item name="android:indeterminateDrawable">@drawable/media_buffering_progressbar</item>
        <item name="android:indeterminateDuration">1200</item>
    </style>

    <!--播放器进度条的styles-->
    <style name="playerSeekbarStyle">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:focusable">true</item>
        <item name="android:progressDrawable">@drawable/player_seekbar_drawable</item>
        <item name="android:thumb">@drawable/seekbar_thumb</item>
        <item name="android:minHeight">4dip</item>
        <item name="android:maxHeight">4dip</item>
    </style>
    <!-- 播放器进度条end-->

    <!--播放器进度条的styles-->
    <style name="playerProgressbarStyle">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:focusable">true</item>
        <item name="android:progressDrawable">@drawable/player_progress_drawable</item>
        <item name="android:minHeight">2dip</item>
        <item name="android:maxHeight">10dip</item>
    </style>
    <!-- 播放器进度条end-->

    <!--音量、亮度progress的styles-->
    <style name="valueProgressbarStyle">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:focusable">true</item>
        <item name="android:progressDrawable">@drawable/vertical_progress_drawable</item>
        <item name="android:minHeight">2dip</item>
        <item name="android:maxHeight">10dip</item>
    </style>
    <!-- 音量、亮度progress的stylesend-->
</resources>